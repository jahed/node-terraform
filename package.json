    "find-cache-directory": "^6.0.0",
    "openpgp": "^6.0.0"
  },
  "devDependencies": {
    "@tsconfig/node20": "^20.1.6",
    "@types/node": "^20.19.17",
    "npm-check-updates": "^16.14.20",
    "prettier": "^2.8.8",
    "typescript": "5.9.x"
  }
}
